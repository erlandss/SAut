<?xml version="1.0"?>
<launch>
    
    <param name="use_sim_time" value="true"/>
    
    <arg name="bag_path" default="/home/erland/catkin_ws/src/fast_slam/ros_bags/"/>
    <arg name="aruco_bag_name" default="run1_aruco"/>
    <arg name="pose_bag_name" default="run1_pose"/>
    
    <node pkg="rosbag" type="play" name="player" output="screen" args="--clock $(arg bag_path)$(arg aruco_bag_name).bag $(arg bag_path)$(arg pose_bag_name).bag"/>
    
</launch>